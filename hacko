-- Scripts:

local function MWGG_fake_script() -- helthesp.LocalScript 
	local script = Instance.new('LocalScript', helthesp)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/anaccountofstuff/box/main/esp%20box'))()
	end)
end
coroutine.wrap(MWGG_fake_script)()
