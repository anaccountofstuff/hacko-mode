local function FCWKFQ_fake_script() -- esptabButton.LocalScript 
	local script = Instance.new('LocalScript', esptabButton)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.aimtab.Visible=false
		script.Parent.Parent.esptab.Visible=true
		script.Parent.Parent.movetab.Visible=false
		script.Parent.Parent.HomeTab.Visible=false
	end)
end
coroutine.wrap(FCWKFQ_fake_script)()
